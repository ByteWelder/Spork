<manifest package="spork.android.proguard"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:label="Spork ProGuard"
        android:name=".Application">

        <activity android:name="spork.android.proguard.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>
